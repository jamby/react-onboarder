{
  "presets": [
    ["es2015", { "modules": false }],
    "react",
    "stage-0"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
    "rollup": {
      "plugins": [
        "external-helpers"
      ]
    },
    "test": {
      "presets": ["es2015", "react", "stage-0"]
    }
  }
}